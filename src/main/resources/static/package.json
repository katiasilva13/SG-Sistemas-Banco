    "scripts": {
        "dev": "node --max-old-space-size=8192 node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --config ./config/webpack.config.dev.js",
        "prod": "NODE_OPTIONS=--max-old-space-size=8000 webpack --progress --colors --config ./config/webpack.config.prod.js --sourcemap=false && node ./build.mobiage.js",
        "electron": "NODE_OPTIONS=--max-old-space-size=8000 webpack --progress --colors --config ./config/webpack.config.electron.js --sourcemap=false",
        "dev:offline": "MB_THEME=mobiage-orange ENV=dev OFFLINE=true npm run dev",
        "prod:offline": "MB_THEME=mobiage-orange ENV=dev OFFLINE=true npm run prod",
        "dev:mobiage": "MB_THEME=mobiage-orange ENV=dev npm run dev",
        "prod:mobiage": "npm rebuild node-sass && MB_THEME=mobiage-orange ENV=prod npm run prod",
        "alpha:mobiage": "npm rebuild node-sass && MB_THEME=mobiage-orange ENV=alpha npm run prod",
        "stage:mobiage": "npm rebuild node-sass && MB_THEME=mobiage-orange ENV=alpha npm run prod",
        "dev:sg": "MB_THEME=sg ENV=dev npm run dev",
        "prod:sg": "npm rebuild node-sass && MB_THEME=sg ENV=prod npm run prod",
        "alpha:sg": "npm rebuild node-sass && MB_THEME=sg ENV=alpha npm run prod",
        "dev:intelivenda": "MB_THEME=mobiage-intelivenda ENV=dev npm run dev",
        "prod:intelivenda": "npm rebuild node-sass && MB_THEME=mobiage-intelivenda ENV=prod npm run prod",
        "alpha:intelivenda": "npm rebuild node-sass && MB_THEME=mobiage-intelivenda ENV=alpha npm run prod",
        "dev:viaweb": "MB_THEME=mobiage-viaweb ENV=dev npm run dev",
        "prod:viaweb": "npm rebuild node-sass && MB_THEME=mobiage-viaweb ENV=prod npm run prod",
        "alpha:viaweb": "npm rebuild node-sass && MB_THEME=mobiage-viaweb ENV=alpha npm run prod",
        "prod:mobiageserver": "npm rebuild node-sass && MB_THEME=mobiageserver ENV=prod npm run prod",
        "build:electron": "npm rebuild node-sass && MB_THEME=mobiage-orange ENV=alpha OFFLINE=true npm run electron && node ./build.electron.js",
        "build:electron-sg": "npm rebuild node-sass && MB_THEME=sg ENV=alpha OFFLINE=true npm run electron && node ./build.electron.js",
        "build:electron-dev": "npm rebuild node-sass && MB_THEME=sg ENV=dev OFFLINE=true npm run electron && node ./build.electron.js",
        "build:electron-prod": "npm rebuild node-sass && MB_THEME=mobiage-orange ENV=prod OFFLINE=true npm run electron && node ./build.electron.js",
        "build:sg:electron-prod": "npm rebuild node-sass && MB_THEME=sg ENV=prod OFFLINE=true npm run electron && node ./build.electron.js",
        "electron:mobiage": "npm run build:electron && cd electron && npm i && npm start; cd..",
        "electron:sg": "npm run build:electron-sg && cd electron && npm i && npm start; cd..",
        "electron:sg-prod": "npm run build:sg:electron-prod && cd electron && npm i && npm start; cd..",
        "electron:sg-dev": "npm run build:electron-dev && cd electron && npm i && npm start; cd.."
    },
